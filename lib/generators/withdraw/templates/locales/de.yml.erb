de:
  withdraw_channel:
    <%= name.underscore %>:
      key: Blockchain
      code: <%= code %>
      title: <%= name %>
      intro: Zahlen Sie <%= name %> von COINDEX zu Ihrem eigenen Wallet aus.
      latency: 30 Minuten
      transfer: Manuell
      go: Jetzt Auszahlen

  activerecord:
    models:
      withdraws/<%= name.underscore %>: Auszahlen
    attributes:
      withdraws/<%= name.underscore %>:
        id: ID
        member_name: Transaktions ID
        currency_obj_key_text: Währung
        state_and_actions: Status
        fund_extra_text: Withdraw Label
        fund_extrat: Withdraw Label
        fund_source: Bank
        fund_uid: <%= name %> Adresse
        created_at: Erstellt am
        sum: Betrag
        amount: Betrag
        remark: Remark
        fee: Gebühren
        state: Status
        state_and_action: Status
    errors:
      models:
        withdraws/<%= name.underscore %>:
          attributes:
            sum:
              poor: Ihr Kontostand ist ungenügend
              greater_than: Ungültiger Auszahlungsbetrag 
            fund_uid:
              blank: Sie haben keine Auszahlungsadresse angegeben
              invalid: Dies ist keine gültige Auszahlungsadresse
              ismine: Dies ist keine gültige Auszahlungsadresse 

  withdraws/<%= name.underscore %>:
    aasm_state:
      submitting: in Bearbeitung
      submitted: in Bearbeitung
      rejected: Abgelehnt
      accepted: in Bearbeitung
      suspect: Suspekt
      processing: in Bearbeitung
      coin_ready: Coin Ready
      coin_done: Coin Done
      done: Erledigt
      almost_done: Almost Done
      canceled: Annuliert
      failed: Fehler

  private:
    withdraws:
      <%= name.underscore.pluralize %>:
        destroy:
          notice: Ihre Auzahlung wurde annuliert und der reservierte Betrag dem Konto wieder gutgeschrieben.
        update:
          notice: Ihr Auszahlungsantrag wurde erfolgreich übermittelt und sobald als Möglich bearbeitet.
          alert_two_factor: Falsches Zwei-Faktoren Authentifizierung Passwort.
        new:
          submit: Ausführen
          allin: All-In
          fund_uid: Gespeicherte Adresse oder Neue erstellen
          fund_extra: 选择开户行
          fund_source_label: <%= name %> Adresse
          manage_fund_source: <%= name %> Adressen verwalten
        create:
          notice: Bitte überprüfen Sie untenstehende Daten und bestätigen Sie die Auzahlung.

  admin:
    withdraws:
      <%= name.underscore.pluralize %>:
        index:
          one: 待处理提现订单
          all: 二十四小时内订单
          empty: 无任何数据
        show:
          process: 受理
          succeed: 提现
          reject: 驳回
          succeed_confirm: 确定处理提现？
          reject_confirm: 确定驳回提现？
          withdraw: 比特币提现

  simple_form:
    labels:
      withdraw:
        <%= name.underscore %>:
          fee: Gebühren
          sum: Betrag
          fund_uid: <%= name %> Adresse
          fund_extra: Memo
          account_balance: Kontostand
          member_name: Konto
    placeholders:
      withdraw:
        <%= name.underscore %>:
          sum: Minimum Betrag 0.001
          fund_extra: Memo für diese Adresse (optional)
    hints:
      withdraw:
        <%= name.underscore %>:
          sum: "<a target='_balance' href='#'>Gebühren</a>"

  guides:
    private:
      withdraws:
        <%= name.underscore.pluralize %>:
          panel: <%= name %> Blockchain
          new:
            panel: Auszahlen
            intro: Geben Sie die <%= name %> Adresse sowie den Betrag an welchen Sie von Ihrem COINDEX Konto auszahlen wollen.
            default-panel: <%= name %> Auszahlungsverlauf
          create:
            panel: Auszahlen
            intro: Geben Sie die <%= name %> Adresse sowie den Betrag an welchen Sie von Ihrem COINDEX Konto auszahlen wollen.
            default-panel: <%= name %> Auszahlungsverlauf
          edit:
            panel: Auszahlen
            intro: Geben Sie die <%= name %> Adresse sowie den Betrag an welchen Sie von Ihrem COINDEX Konto auszahlen wollen.
            default-panel: <%= name %> Auszahlungsverlauf

