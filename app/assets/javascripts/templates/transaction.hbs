  <td class="type">{{t type}}</td>
  <td class="timestamp">{{format_long_time timestamp}}</td>
  <td class="fiat_currency">{{t fiat_currency}}</td>
  <td class="fiat_amount">{{fiat_amount}}</td>
  <td class="coin_currency">{{t coin_currency}}</td>
  <td class="coin_amount">{{format_fix_bid coin_amount}}</td>
  <td class="coin_price">{{format_fix_bid coin_price}}</td>
  <td class="fee">{{format_fix_bid fee}}</td>
</tr>
